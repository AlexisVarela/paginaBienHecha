<link rel="stylesheet" href="/styles/cardsCortes.css">
<div class="contendorPirncipal">
    <h2>Cortes disponibles en el local</h2>
    <div class="cards">
        <% cortes.forEach(corte => { %>
            <div class="card">
                <div class="imgn"><img src="/img/cortes/<%= corte.imagen %>" alt="imagen del corte <%= corte.corte%>"></div>
                <p>Corte: <%= corte.corte %></p>
                <p>Precio: <%= corte.precio %></p>
            </div>
        <% }); %>
    </div>
</div>